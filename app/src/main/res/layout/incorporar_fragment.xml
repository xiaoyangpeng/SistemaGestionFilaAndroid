<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".fragments.incorporar.IncorporarFragment">

    <ImageView
        android:id="@+id/escannear"
        android:layout_width="217dp"
        android:layout_height="212dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/personalizado_boton_login"
        android:src="@drawable/escannear"
        app:layout_constraintBottom_toTopOf="@+id/remota"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.803" />

    <ImageView
        android:id="@+id/remota"
        android:layout_width="352dp"
        android:layout_height="237dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/personalizado_boton_remota"
        android:src="@drawable/incorporarremota"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>